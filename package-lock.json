{
  "name": "monorepo-demo",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "version": "1.0.0",
      "license": "ISC",
      "workspaces": [
        "packages/*"
      ],
      "engines": {
        "npm": ">=7.0.0"
      }
    },
    "node_modules/@demo/package1": {
      "resolved": "packages/package1",
      "link": true
    },
    "node_modules/@demo/package2": {
      "resolved": "packages/package2",
      "link": true
    },
    "packages/package1": {
      "name": "@demo/package1",
      "version": "1.0.0",
      "license": "ISC"
    },
    "packages/package2": {
      "version": "1.0.0",
      "license": "ISC"
    }
  },
  "dependencies": {
    "@demo/package1": {
      "version": "file:packages/package1"
    },
    "@demo/package2": {
      "version": "file:packages/package2"
    }
  }
}
